    "keywords": ["halda", "taximeter"],
    "homepage": "https://github.com/szeidler/halda-php-sdk",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle-services": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Zeidler",
            "email": "info@stephanzeidler.de"
        }
    ],
    "autoload": {
        "psr-4": {
